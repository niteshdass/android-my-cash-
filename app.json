{
  "expo": {
    "name": "wallet",
    "slug": "wallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/my-logo.png",
    "splash": {
      "image": "./assets/app-loader.png",
      "resizeMode": "contain",
      "backgroundColor": "#2b7fed"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/my-logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.bindaas.wallet"
    },
    "web": {
      "favicon": "./assets/my-logo.png"
    }
  }
}
